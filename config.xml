<?xml version="1.0" encoding="utf-8"?>
<flex-config>
  <target-player>14.0</target-player>
  <benchmark>false</benchmark>
  <static-link-runtime-shared-libraries>true</static-link-runtime-shared-libraries>
  <compiler>
    <define append="true">
      <name>CONFIG::debug</name>
      <value>true</value>
    </define>
    <define append="true">
      <name>CONFIG::release</name>
      <value>false</value>
    </define>
    <define append="true">
      <name>CONFIG::timeStamp</name>
      <value>'7/18/2017'</value>
    </define>
    <define append="true">
      <name>CONFIG::air</name>
      <value>true</value>
    </define>
    <define append="true">
      <name>CONFIG::mobile</name>
      <value>false</value>
    </define>
    <define append="true">
      <name>CONFIG::desktop</name>
      <value>true</value>
    </define>
    <verbose-stacktraces>true</verbose-stacktraces>
    <source-path append="true">
      <path-element>./src/main</path-element>
      <path-element>./src/test</path-element>
    </source-path>
    <external-library-path append="true">
      <path-element>./lib/OpenFinAirNativeExtension-6.0.1.ane</path-element>
    </external-library-path>
    <library-path append="true">
      <path-element>./lib/flexunit-4.2.0-20140410-as3_4.12.0.swc</path-element>
      <path-element>./lib/flexunit-4.2.0-20140410-flex_4.12.0.swc</path-element>
      <path-element>./lib/flexunit-aircilistener-4.2.0-20140410-4.12.0.swc</path-element>
      <path-element>./lib/flexunit-cilistener-4.2.0-20140410-4.12.0.swc</path-element>
      <path-element>./lib/flexunit-uilistener-4.2.0-20140410-4.12.0.swc</path-element>
      <path-element>./lib/fluint-1_2.swc</path-element>
      <path-element>./lib/fluint-extensions-4.2.0-20140410-4.12.0.swc</path-element>
      <path-element>./lib/OpenFinAirAdapter-6.0.1.swc</path-element>
    </library-path>
  </compiler>
  <file-specs>
    <path-element>./src/test/TestRunner.as</path-element>
  </file-specs>
  <default-background-color>#FFFFFF</default-background-color>
  <default-frame-rate>30</default-frame-rate>
  <default-size>
    <width>800</width>
    <height>600</height>
  </default-size>
</flex-config>
